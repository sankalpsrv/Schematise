<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE RDF [
   <!ENTITY dcterms "http://purl.org/dc/terms">  
   <!ENTITY lrmlmm "http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel">    
   <!ENTITY lrmlv "http://docs.oasis-open.org/legalruleml/ns/v1.0/vocab">  
   <!ENTITY owl "http://www.w3.org/2002/07/owl">    
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns">  
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema">  
   <!ENTITY rulemm "http://docs.oasis-open.org/legalruleml/ns/v1.0/rule-metamodel">  
   <!ENTITY xs "http://www.w3.org/2001/XMLSchema">    
 ]>
<rdf:RDF 
  xmlns:lrmlmm="&lrmlmm;#" 
  xmlns:owl="&owl;#"
  xmlns:rdf="&rdf;#" 
  xmlns:rdfs="&rdfs;#" 
  xmlns:rulemm="&rulemm;#" 
  xmlns:xs="&xs;"
  xml:base="&lrmlmm;">
  <!--<owl:Ontology rdf:about="&lrmlmm;/modules/metadata.rdfs" dc:title="The LegalRuleML MetaModel Metadata vocabulary"/>-->
  

  
  <rdfs:Class rdf:about="#Authorities">
    <rdfs:label>authority collection</rdfs:label>
    <rdfs:comment>a collection where every member is an Authority</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Collection"/>    
  </rdfs:Class>
  
  <rdfs:Class rdf:about="#Authority">
    <rdfs:label>authority</rdfs:label>
    <rdfs:comment>a person or organization with the power to create, endorse, or enforce Legal Norms.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Associable"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="#Jurisdiction">
    <rdfs:label>jurisdiction</rdfs:label>
    <rdfs:comment>a geographic area or subject-matter over which an Authority applies its legal power.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Associable"/>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="http://purl.org/dc/terms/Jurisdiction">
      </rdfs:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Concept"/>    
  </rdfs:Class>

  <rdfs:Class rdf:about="#Jurisdictions">
    <rdfs:label>jursidiction collection</rdfs:label>
    <rdfs:comment>a collection where every member is a Jurisdiction.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Collection"/>    
  </rdfs:Class>
    
  <rdf:Property rdf:about="#appliesAuthority">
    <rdfs:label>applies authority</rdfs:label>
    <rdfs:comment>an Authority applied by the Context or Association.</rdfs:comment>
    <rdfs:range rdf:resource="#Authority"/>
    <rdfs:subPropertyOf rdf:resource="#applies"/>
  </rdf:Property>
    
  <rdf:Property rdf:about="#appliesJurisdiction">
    <rdfs:label>applies jurisdiction</rdfs:label>
    <rdfs:comment>a Jurisdiction applied by the Context or Association.</rdfs:comment>
    <rdfs:range rdf:resource="#Jurisdiction"/>    
    <rdfs:subPropertyOf rdf:resource="#applies"/>
  </rdf:Property>
  
  
  <rdf:Property rdf:about="#hasAuthorities">
    <rdfs:label>has authority collection</rdfs:label>
    <rdfs:comment>A collection of authorities associated with the
      subject LegalRuleML document.</rdfs:comment>
    <rdfs:domain rdf:resource="#LegalRuleMLDocument"/>
    <rdfs:range rdf:resource="#Authorities"/>
    <rdfs:subPropertyOf rdf:resource="#hasCollection"/>    
  </rdf:Property>
  
  <rdf:Property rdf:about="#hasAuthority">
    <rdf:label>has member authority</rdf:label>
    <rdfs:comment>An authority which is a member of the subject collection.</rdfs:comment>
    <rdfs:domain rdf:resource="#Authorities"/>
    <rdfs:range rdf:resource="#Authority"/>
    <rdfs:subPropertyOf rdf:resource="#hasMember"/>    
  </rdf:Property>
  
  
  <rdf:Property rdf:about="#hasJurisdiction">
    <rdf:label>has member jurisdiction</rdf:label>
    <rdfs:comment>a Jurisdiction in the collection.</rdfs:comment>
    <rdfs:domain rdf:resource="#Jurisdictions"/>
    <rdfs:range rdf:resource="#Jurisdiction"/>
    <rdfs:subPropertyOf rdf:resource="#hasMember"/>    
  </rdf:Property>

  <rdf:Property rdf:about="#hasJurisdictions">
    <rdfs:label>has jurisdiction collection</rdfs:label>
    <rdfs:comment>a collection of Jurisdictions.</rdfs:comment>
    <rdfs:domain rdf:resource="#LegalRuleMLDocument"/>
    <rdfs:range rdf:resource="#Jurisdictions"/>
    <rdfs:subPropertyOf rdf:resource="#hasCollection"/>    
  </rdf:Property>

  <!-- Duplicate from context.rdfs for diagramming purposes -->
  <rdf:Property rdf:about="#applies">
    <rdfs:domain rdf:resource="#Associator"/>
    <rdfs:range rdf:resource="#Associable"/>
  </rdf:Property>
  
</rdf:RDF>
